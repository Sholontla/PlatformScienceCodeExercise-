
// POST USER
curl -X POST 127.0.0.1:3000/user -d '{"username":"user1", "password":"pass"}'

// GET ALL USERS
curl 127.0.0.1:3000/users

// GET USER BY USERNAME
curl 127.0.0.1:3000/get/user/user1 

// UPDATE USER BY USERNAME 
curl -X PUT 127.0.0.1:3000/update/user/user1 -d '{"username":"user1", "password":"pass"}'

// DELETE USER BY USERNAME 
curl -X DELETE 127.0.0.1:3000/delete/user/user1